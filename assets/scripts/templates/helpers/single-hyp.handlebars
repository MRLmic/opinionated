{{#each hypotheses as |hypothesis|}}
<h3 class="hyp-title" data-id="{{hypothesis.id}}" contenteditable="false"><a data-id="{{hypothesis.id}}" class="hyp-select">{{hypothesis.title}}</a><section class="button-group"><button type="button" name="button" class="btn btn-danger btn-xs"><span class="glyphicon glyphicon-remove"></span></button>
<button type="button" data-toggle="tooltip" title="click once then click on text to edit, then click again to submit" name="button" class="edit btn btn-primary btn-xs"><span class="glyphicon glyphicon-pencil" aria-hidden="true"></span></button>
</section></h3>
{{/each}}
